{
  "name": "tamaru",
  "description": "tamaru",
  "version": "1.0.0",
  "author": "bouzuya <m@bouzuya.net> (http://bouzuya.net/)",
  "bugs": {
    "url": "https://github.com/bouzuya/tamaru/issues"
  },
  "dependencies": {
    "googleapis": "^30.0.0"
  },
  "devDependencies": {
    "bower": "^1.8.4",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.2",
    "pulp": "^12.2.0",
    "purescript": "^0.11.7",
    "rimraf": "^2.6.2"
  },
  "homepage": "https://github.com/bouzuya/tamaru#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouzuya/tamaru.git"
  },
  "scripts": {
    "bower:install": "bower install",
    "build": "npm-run-all -s build:server",
    "build:server": "pulp build --main Server.Main",
    "clean": "rimraf .pulp-cache output",
    "prepare": "npm-run-all -s bower:install clean build",
    "repl": "pulp repl",
    "start": "node --eval \"require('./output/Server.Main').main();\"",
    "test": "pulp test",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:server": "pulp --watch build --main Server.Main",
    "watch:start": "nodemon --watch output --exec 'npm start'"
  }
}
